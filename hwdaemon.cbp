<?xml version="1.0" encoding="UTF-8" standalone="yes" ?>
<CodeBlocks_project_file>
	<FileVersion major="1" minor="6" />
	<Project>
		<Option title="hwdaemon" />
		<Option pch_mode="2" />
		<Option compiler="gcc" />
		<Build>
			<Target title="x86">
				<Option output="bin/x86/hwdaemon" prefix_auto="1" extension_auto="1" />
				<Option object_output="obj/x86/" />
				<Option type="1" />
				<Option compiler="gcc" />
				<Compiler>
					<Add option="-g" />
				</Compiler>
				<Linker>
					<Add option="-lm" />
					<Add option="-lgps" />
					<Add option="-pthread" />
				</Linker>
			</Target>
			<Target title="Beagle">
				<Option output="bin/Beagle/hwdaemon" prefix_auto="1" extension_auto="1" />
				<Option object_output="obj/Beagle/" />
				<Option type="1" />
				<Option compiler="gcc_c_cross_beaglebone" />
				<Compiler>
					<Add option="-O2" />
				</Compiler>
				<Linker>
					<Add option="-s" />
				</Linker>
				<ExtraCommands>
					<Add after="cp ${TARGET_OUTPUT_FILE} /tmp/sshfs" />
				</ExtraCommands>
			</Target>
		</Build>
		<Compiler>
			<Add option="-std=c++11" />
			<Add option="-Wall" />
			<Add option="-fexceptions" />
		</Compiler>
		<Linker>
			<Add option="-lpthread" />
			<Add option="-lm" />
		</Linker>
		<Unit filename="Adc.cpp" />
		<Unit filename="Adc.hpp" />
		<Unit filename="Pwm.cpp" />
		<Unit filename="Pwm.hpp" />
		<Unit filename="main.cpp" />
		<Unit filename="socket.c">
			<Option compilerVar="CC" />
		</Unit>
		<Unit filename="socket.h" />
		<Unit filename="utils.cpp" />
		<Unit filename="utils.hpp" />
		<Extensions>
			<code_completion />
			<envvars />
			<debugger />
			<lib_finder disable_auto="1" />
		</Extensions>
	</Project>
</CodeBlocks_project_file>
